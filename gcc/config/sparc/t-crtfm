EXTRA_PARTS += crtfastmath.o gccbuiltins.il

$(T)crtfastmath.o: $(srcdir)/config/sparc/crtfastmath.c $(GCC_PASSES)
	$(GCC_FOR_TARGET) $(MULTILIB_CFLAGS) $(LIBGCC2_CFLAGS) -c -o $(T)crtfastmath.o $(srcdir)/config/sparc/crtfastmath.c

$(T)gccbuiltins.il: $(srcdir)/config/sparc/gccbuiltins.il $(srcdir)/config/sparc/gccbuiltins-m64.il specs
	$(INSTALL_DATA) $(srcdir)/config/sparc/gccbuiltins.il gccbuiltins.il
	mkdir sparcv9
	$(INSTALL_DATA) $(srcdir)/config/sparc/gccbuiltins-m64.il sparcv9/gccbuiltins.il

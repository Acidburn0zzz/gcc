#!/bin/ksh

if [ $# -ne 1 ]
then
	cat >&2 <<EOF
Usage: $0 <testdir>
where <testdir> is the root directory where the hydra tests were run.
EOF
	exit 1
fi

ROOTDIR=$1

numrundirs=`/bin/ls $ROOTDIR | wc -l`
if [ $numrundirs -ne 1 ]
then
	echo >&2 "WARN: Expected exactly one directory under $ROOTDIR"
fi

sort $ROOTDIR/*/*/fail_list | awk '{print $1}'
